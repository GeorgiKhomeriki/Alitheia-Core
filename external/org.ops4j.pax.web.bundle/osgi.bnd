Bundle-SymbolicName: org.ops4j.pax.web.pax-web-bundle
 
Bundle-Version>:\
  ${pom.version}

Bundle-Activator:\
  org.ops4j.pax.web.service.internal.Activator

Import-Package:\
  javax.servlet.*; version="[2.3.0,3.0.0)",\
  org.apache.commons.logging; version="1.0.4"; resolution:=optional,\
  org.mortbay.*,\
  org.osgi.framework; version="[1.0.0,2.0.0)",\
  org.osgi.service.cm; version="[1.0.0,2.0.0)",\
  org.osgi.service.http; version="[1.0.0,2.0.0)",\
  org.ops4j.pax.web.jsp;resolution:=optional 

Export-Package:\
  org.ops4j.pax.web.service,\
  org.osgi.service.http; version="1.2.0"

Embed-Dependency:\
  *; artifactId=!org.osgi.compendium; scope=compile; type=!pom; inline=true

Embed-Transitive: true

-removeheaders:\
  Include-Resource,\
  Private-Package,\
  Embed-Dependency,\
  Embed-Transitive

-nouses:true
