#--------------------------------------------------------------------------
# Use this file to add customized Bnd instructions for the wrapped library
#--------------------------------------------------------------------------

#
# this unpacks the contents of the wrapped jar artifact inside the bundle
# to also inline dependencies of this artifact add Embed-Transitive: true
#
#Embed-Dependency: *;scope=compile|runtime;type=!pom;inline=true
#Import-Package: !org.ops4j.*, org.ops4j.pax.web.service.internal, org.ops4j.pax.web.jsp.*, *

Bundle-SymbolicName:\
  ${bundle.symbolicName}

Bundle-Version>:\
  ${pom.version}

Bundle-Activator:\
  org.ops4j.pax.web.service.internal.Activator

Import-Package:\
  javax.servlet.*; version="[2.3.0,3.0.0)",\
  org.apache.commons.logging; version="1.0.4"; resolution:=optional,\
  org.mortbay.*;version="[6.1.0, 7.0.0)",\
  org.osgi.framework; version="[1.0.0,2.0.0)",\
  org.osgi.service.cm; version="[1.0.0,2.0.0)",\
  org.osgi.service.http; version="[1.0.0,2.0.0)",\
  org.ops4j.pax.web.jsp;resolution:=optional 

Export-Package:\
  org.ops4j.pax.web.service,\
  org.osgi.service.http; version="1.2.0"

Embed-Dependency:\
  *; artifactId=!org.osgi.compendium; scope=compile; type=!pom; inline=true

Embed-Transitive: true

-removeheaders:\
  Include-Resource,\
  Private-Package,\
  Embed-Dependency,\
  Embed-Transitive

-nouses:true
