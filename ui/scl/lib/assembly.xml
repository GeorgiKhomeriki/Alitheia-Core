<assembly>
  <id>sqooss.scl.ws.client</id>
  <includeBaseDirectory>false</includeBaseDirectory>
  <files>
    <file>
      <source>${basedir}/target/client-${version}.jar</source>
      <destName>eu.sqooss.ws.${artifactId}_${version}.${packaging}</destName>
      <!-- Assembly 2.1 uses an octal representation of file modes, i.e.
        (dec) 420 corresponds to (oct) 644 -->
      <fileMode>420</fileMode>
    </file>
  </files>
</assembly>