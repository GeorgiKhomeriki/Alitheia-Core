<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.metrics.mde.db.MDEWeek" table="MDE_WEEK">
    <id name="id" column="MDE_WEEK_ID">
      <generator class="native" />
    </id>
    <many-to-one name="developer"
        column="DEVELOPER_ID"
        not-null="true"
        cascade="all" />
    <property name="week" column="WEEK" not-null="true" />
    <property name="active" column="ACTIVE" not-null="true" />
  </class>
</hibernate-mapping>
