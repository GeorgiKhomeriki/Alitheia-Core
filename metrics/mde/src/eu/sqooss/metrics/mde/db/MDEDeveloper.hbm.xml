<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.metrics.mde.db.MDEDeveloper" table="MDE_DEVELOPER">
    <id name="id" column="MDE_DEVELOPER_ID">
      <generator class="foreign">
          <param name="property">developer</param>
      </generator>
    </id>
    <many-to-one name="developer" 
        column="DEVELOPER_ID" 
        not-null="true" 
        cascade="all" 
        unique="true" />
    <property name="start" column="START_TIME" not-null="true" />
    <property name="startWeek" column="START_WEEK" not-null="true" />
  </class>
</hibernate-mapping>
