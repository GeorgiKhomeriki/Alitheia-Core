<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.metrics.simulation.db.SimulationResults" table="SIMULATION_RESULTS">
    <id name="id" column="id">
      <generator class="native" />
    </id>
    <property name="time" column="time" not-null="true" type = "long" />
    <property name="runtime" column="runtime" not-null="true" type = "long" />
    <property name="loc" column="loc" not-null="true" type="double" />
    <property name="bDens" column="b_dens" not-null="true" type="double" />
    <property name="bRepDens" column="b_rep_dens" not-null="true" type="double" />
    <property name="q" column="q" not-null="true" type="double" />
    <property name="totS" column="tots" not-null="true" type="double" />
    <property name="totB" column="totb" not-null="true" type="double" />
    <property name="totT" column="tott" not-null="true" type="double" />
    <property name="totF" column="totF" not-null="true" type="double" />
    <property name="occProgrammers" column="occ_programmers" not-null="true" type="double" />
    <property name="newBlood" column="new_blood" not-null="true" type="double" />
    <property name="loc2" column="loc2" not-null="true" type="double" />
    <property name="bDens2" column="b_dens2" not-null="true" type="double" />
    <property name="bRepDens2" column="b_rep_dens2" not-null="true" type="double" />
    <property name="q2" column="q2" not-null="true" type="double" />
    <property name="totS2" column="tots2" not-null="true" type="double" />
    <property name="totB2" column="totb2" not-null="true" type="double" />
    <property name="totT2" column="tott2" not-null="true" type="double" />
    <property name="totF2" column="totF2" not-null="true" type="double" />
    <property name="occProgrammers2" column="occ_programmers2" not-null="true" type="double" />
    <property name="newBlood2" column="new_blood2" not-null="true" type="double" />
    
        
  </class>
</hibernate-mapping>
