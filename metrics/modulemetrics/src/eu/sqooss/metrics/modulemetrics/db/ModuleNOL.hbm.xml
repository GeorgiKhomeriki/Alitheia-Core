<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.metrics.modulemetrics.db.ModuleNOL" table="MODULE_NOL">
    <id name="id" column="MODULE_NOL_ID">
      <generator class="native" />
    </id>
    <property name="result" column="RESULT" type="string" />
    <many-to-one name="projectFile" column="PROJECT_FILE_ID" not-null="true" cascade="merge" />
    <many-to-one name="projectVersion" column="PROJECT_VERSION_ID" not-null="true" cascade="merge" />
  </class>
</hibernate-mapping>
