<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.metrics.productivity.db.Actions" table="PRODUCTIVITY_ACTIONS">
    <id name="id" column="ACTION_ID">
      <generator class="native" />
    </id>
    <property name="resourceTypeID" column="RESOURCE_TYPE_ID" />
    <property name="actionName" column="ACTION_NAME" />
    <property name="actionProductive" column="IS_ACTION_PRODUCTIVE" />
  </class>
</hibernate-mapping>
