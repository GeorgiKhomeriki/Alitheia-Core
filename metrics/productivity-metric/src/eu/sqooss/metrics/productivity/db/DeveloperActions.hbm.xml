<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.metrics.productivity.db.DeveloperActions" table="PRODUCTIVITY_DEVELOPER_ACTIONS">
    <id name="id" column="DEVELOPER_ACTION_ID">
      <generator class="native" />
    </id>
    <property name="developerID" column="DEVELOPER_ID" />
    <property name="weightID" column="WEIGHT_ID" />
    <property name="totalActions" column="TOTAL_ACTIONS" />
  </class>
</hibernate-mapping>
