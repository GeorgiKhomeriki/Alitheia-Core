<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="eu.sqooss.db">
  <class name="ProjectVersion" table="PROJECT_VERSION">
    <id name="id" column="ID">
      <generator class="native"/>
    </id>
    <many-to-one name="storedProject"
		 class="StoredProject"
		 column="STORED_PROJECT_ID"
		 not-null="true"/>
    <property name="version" column="VERSION"/>
    <property name="localPath" column="LOCAL_PATH"/>
  </class>
</hibernate-mapping>