<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.Commit" table="COMMIT">
    <id name="id" column="COMMIT_ID">
      <generator class="native" />
    </id>
    <property name="commitTime" column="COMMIT_TIME" />
    <property name="committer" column="COMMITTER" />
    <property name="projectVersion" column="PROJECT_VERSION" />
    <property name="properties" column="PROPERTIES" />
  </class>
</hibernate-mapping>
