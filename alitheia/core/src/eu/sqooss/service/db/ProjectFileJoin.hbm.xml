<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.ProjectFileJoin" table="PROJECT_FILE_JOIN">
    <id name="id" column="PROJECT_FILE_JOIN_ID">
      <generator class="native" />
    </id>
    <many-to-one name="projectFile" column="PROJECT_FILE_ID" not-null="true" />
    <many-to-one name="projectVersion" column="PROJECT_VERSION_ID" not-null="true"/>
  </class>
</hibernate-mapping>
