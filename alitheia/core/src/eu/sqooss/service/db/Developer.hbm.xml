<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.Developer" table="Developer">
    <id name="id" column="DEVELOPER_ID">
      <generator class="native" />
    </id>
    
    <property name="name" column="NAME" />
    <property name="email" column="EMAIL" unique="true" />
    <property name="username" column="USERNAME" unique-key="unq.uname.sp"/>
    <many-to-one name="storedProject" column="STORED_PROJECT_ID" unique-key="unq.uname.sp"/>
  </class>
</hibernate-mapping>
