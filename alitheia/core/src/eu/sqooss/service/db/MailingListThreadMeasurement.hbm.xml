<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.MailingListThreadMeasurement" table="ML_THREAD_MEASUREMENT">
    <id name="id" column="ML_THREAD_MEASUREMENT_ID">
      <generator class="native" />
    </id>
    <many-to-one name="metric" column="METRIC_ID" not-null="true" cascade="merge" />
    <many-to-one name="thread" column="THREAD_ID" not-null="true" cascade="merge" />
    <property name="result" column="RESULT" type="string" />
  </class>
</hibernate-mapping>
