<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.BugPriority" table="BUG_PRIORITY">
    <id name="id" column="BUG_PRIORITY_ID">
      <generator class="native" />
    </id>
    <property name="priority" column="PRIORITY" unique="true"/>
  </class>
</hibernate-mapping>
