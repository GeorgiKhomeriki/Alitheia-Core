<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.BugStatus" table="BUG_STATUS">
    <id name="id" column="BUG_STATUS_ID">
      <generator class="native" />
    </id>
    <property name="status" column="STATUS" unique="true"/>
  </class>
</hibernate-mapping>
