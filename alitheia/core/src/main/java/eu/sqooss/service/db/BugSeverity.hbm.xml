<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.BugSeverity" table="BUG_SEVERITY">
    <id name="id" column="BUG_SEVERITY_ID">
      <generator class="native" />
    </id>
    <property name="severity" column="SEVERITY" unique="true"/>
  </class>
</hibernate-mapping>
