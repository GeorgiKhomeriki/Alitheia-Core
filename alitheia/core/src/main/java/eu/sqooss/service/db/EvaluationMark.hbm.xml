<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.EvaluationMark" table="EVALUATION_MARK">
    <id name="id" column="EVALUATION_MARK_ID">
      <generator class="native" />
    </id>
    <many-to-one name="metric" column="METRIC_ID" not-null="true" cascade="merge" />
    <many-to-one name="storedProject" column="STORED_PROJECT_ID" not-null="true" cascade="merge"/>
    <many-to-one name="version" column="PROJECT_VERSION_ID" not-null="false" cascade="merge" />
    <property name="whenRun" column="WHEN_RUN" type="java.util.Date" />
  </class>
</hibernate-mapping>
