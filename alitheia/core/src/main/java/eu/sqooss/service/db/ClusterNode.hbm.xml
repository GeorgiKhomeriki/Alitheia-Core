<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="eu.sqooss.service.db.ClusterNode" table="CLUSTERNODE">
    <id name="id" column="CLUSTERNODE_ID">
      <generator class="native" />
    </id>
    <property name="name" column="CLUSTERNODE_NAME" unique="true" not-null="true" />
     
  </class>
</hibernate-mapping>
