<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="eu.sqooss.service.db.StoredProjectConfig" table="STORED_PROJECT_CONFIG">
    <id name="id" column="STORED_PROJECT_CONFIG_ID" >
      <generator class="native" />
    </id>
    <property name="value" column="VALUE" not-null="true" unique-key="unq" />
    <many-to-one name="project" column="STORED_PROJECT_ID" not-null="true" unique-key="unq"/>
    <many-to-one name="confOpt" column="CONFIG_OPTION_ID" not-null="true" unique-key="unq"/>
  </class>
</hibernate-mapping>
