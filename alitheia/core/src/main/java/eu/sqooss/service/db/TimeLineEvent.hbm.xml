<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.TimeLineEvent" table="TIMELINE">
    <id name="id" column="USER_ID">
      <generator class="native" />
    </id>
    
    <property name="sequenceNum" column="SEQ_NUM" not-null="true" />
    <property name="timestamp" column="TIMESTAMP" not-null="true" />
    <property name="eventId" column="EVENT_ID" not-null="true" />
    <property name="type" length="30" column="EVENT_TYPE" type="string"> 
		<!-- <type name="org.hibernate.type.EnumType"> 
		<param name="enumClass">eu.sqooss.service.db.TimeLineEventType</param> 
		<param name="type">12</param> 
		</type>--> 
	</property>
	<many-to-one name="project" column="STORED_PROJECT_ID" not-null="true" cascade="merge" /> 
  </class>
</hibernate-mapping>

