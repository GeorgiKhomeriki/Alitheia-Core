<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.ProjectFileState" table="PROJECT_FILE_STATE">
    <id name="id" column="PROJECT_FILE_STATE_ID">
      <generator class="native" />
    </id>
    <property name="status" column="STATUS" not-null="true" />
    <set name="files" inverse="true">
        <key column="STATE_ID"/>
        <one-to-many class="eu.sqooss.service.db.ProjectFile"/>
    </set>
  </class>
</hibernate-mapping>
