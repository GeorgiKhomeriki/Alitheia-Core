<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.BugResolution" table="BUG_RESOLUTION">
    <id name="id" column="BUG_RESOLUTION_ID">
      <generator class="native" />
    </id>
    <property name="resolution" column="RESOLUTION" unique="true"/>
  </class>
</hibernate-mapping>
